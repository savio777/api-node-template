{
  "name": "api_exemplo",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "savio777",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --no-notify --exit-child --transpile-only --respawn --ignore-watch node_modules ./src ",
    "build": "rm -rf ./dist && tsc --build tsconfig.build.json",
    "lint": "eslint . && yarn tsc --noEmit",
    "test": "dotenv -e .env jest --runInBand",
    "migration": "dotenv -e .env -- ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/database/data-source.ts",
    "migration:rollback": "dotenv -e .env -- ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/database/data-source.ts",
    "ex:migration:create": "dotenv -e .env -- ts-node ./node_modules/typeorm/cli.js migration:create ./src/database/migrations/CreateExemplo -d ./src/database/data-source.ts"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "jwk-to-pem": "^2.0.5",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/node": "^18.14.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.4",
    "sqlite3": "^5.1.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
